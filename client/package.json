{
	"name": "GiFT",
	"version": "1.0.0",
	"description": "GiFT app",
	"main": "webpack.config.dev.babel.js",
	"dependencies": {
		"axios": "^0.19.0",
		"grommet": "^1.5.0",
		"history": "^4.6.3",
		"lodash": "^4.17.4",
		"prop-types": "^15.5.10",
		"react": "^15.6.1",
		"react-dom": "^15.6.1",
		"react-redux": "^5.0.5",
		"react-router": "^3.0.5",
		"react-router-redux": "^5.0.0-alpha.6",
		"redux": "^3.7.2",
		"redux-thunk": "^2.2.0",
		"schema-utils": "^0.3.0"
	},
	"scripts": {
		"start": "npm-run-all test lint:watch dev",
		"dev": "webpack-dev-server --port 4090 --hot --inline --config webpack.config.dev.babel.js",
		"lint": "eslint src --fix",
		"lint:watch": "npm run lint --watch",
		"build": "webpack --config webpack.config.dist.babel.js",
		"test": "jest",
		"testcoverage": "jest --coverage --no-cache",
		"jestUpdateSnapshots": "jest --updateSnapshot",
		"sample-e2e": "testcafe chrome src/sample-e2e/**/*.e2e.js",
		"e2e": "testcafe --skip-js-errors chrome src/components/**/*.e2e.js --speed 1",
		"e2ed": "testcafe --skip-js-errors chrome src/components/**/*.e2e.js --speed 1 -d",
		"e2eh": "testcafe --skip-js-errors chrome:headless src/components/**/*.e2e.js --speed 1 -S -s build/screenshots",
		"e2ehx": "testcafe --skip-js-errors chrome:headless src/components/**/*.e2e.js --speed 1 -r xunit -S -s build/screenshots > e2e-results.xml",
		"e2es": "testcafe --skip-js-errors chrome src/components/**/*.e2e.js --speed 0.5",
		"e2ei": "testcafe --skip-js-errors chrome:emulation:device=iphone 6 src/components/**/*.e2e.js --speed 0.5"
	},
	"keywords": [],
	"author": "GFT",
	"license": "ISC",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "6.24.1",
		"babel-jest": "^20.0.3",
		"babel-loader": "7.0.0",
		"babel-plugin-react-display-name": "2.0.0",
		"babel-polyfill": "^6.23.0",
		"babel-preset-es2015": "6.24.1",
		"babel-preset-react": "6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"babel-register": "^6.24.1",
		"css-loader": "0.28.4",
		"enzyme": "^2.9.1",
		"eslint": "^6.0.1",
		"eslint-plugin-import": "^2.18.0",
		"eslint-plugin-react": "^7.14.2",
		"eslint-watch": "^5.1.2",
		"html-webpack-plugin": "^2.29.0",
		"jest": "^20.0.4",
		"jest-html-reporter": "^0.5.1",
		"jest-junit": "^2.1.0",
		"node-sass": "^4.5.3",
		"npm": "^5.3.0",
		"npm-run-all": "^4.0.2",
		"react-addons-test-utils": "^15.6.0",
		"react-hot-loader": "^1.3.1",
		"react-test-renderer": "^15.6.1",
		"sass-loader": "^6.0.6",
		"style-loader": "0.18.1",
		"testcafe": "^1.3.1",
		"testcafe-react-selectors": "3.1.0",
		"url-loader": "^0.5.9",
		"webpack": "^3.1.0",
		"webpack-cli": "^3.3.5",
		"webpack-dev-server": "^2.5.1"
	},
	"jest": {
		"testRegex": "\\.spec\\.js$",
		"testResultsProcessor": "./node_modules/jest-junit",
		"bail": true,
		"collectCoverageFrom": [
			"src/components/**/*.{js,jsx}",
			"!**/*.spec.*",
			"!**/e2e/*",
			"!**/node_modules/**",
			"!**/vendor/**"
		],
		"coverageDirectory": "build/coverage"
	},
	"jest-junit": {
		"suiteName": "Front end unit tests",
		"output": "./build/unit-test-results/results.xml",
		"classNameTemplate": "{classname}-{title}",
		"titleTemplate": "{classname}-{title}",
		"ancestorSeparator": " â€º ",
		"usePathForSuiteName": "true"
	},
	"jest-html-reporter": {
		"pageTitle": "Front end unit tests",
		"outputPath": "./build/unit-test-results/index.html",
		"includeFailureMsg": false
	}
}
